@{
    ViewBag.Title = "FileUpload";
}

<form action="" method="post" enctype="multipart/form-data">
<table id="tblList">
    <caption>
        Upload new file
    </caption>
    <tbody>
        <tr>
            <td>
                <input type="file" id="file" name="file" />
                <input type="submit" value="Submit" />
            </td>
        </tr>
    </tbody>
</table>
</form>
